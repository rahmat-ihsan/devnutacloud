.nuta-link-box-menu {
    color: #454545;
    text-decoration: none !important;

}

.nuta-link-box-menu .iconic-w-horizontal, .nuta-link-box-menu .iconic-w-wrap {
    /* -webkit-border-radius: 4px; */
    -moz-border-radius: 4px;
    -ms-border-radius: 4px;
    -o-border-radius: 4px;
    /* border-radius: 4px; */
    /* background: #ececec; */
    /* background-size: 100%; */
    background-image: -webkit-gradient(linear, 50% 0, 50% 100%, color-stop(0%, #fafafa), color-stop(100%, #ececec));
    background-image: -webkit-linear-gradient(top, #fafafa, #ececec);
    background-image: linear-gradient(top, #fafafa, #ececec) !important;
    background-image: -moz-linear-gradient(top, #fafafa, #ececec) !important;
    background-image: -o-linear-gradient(top, #fafafa, #ececec) !important;

    border: 1px solid #d5d5d5 !important;
    -webkit-box-shadow: 0 0 0 1px #fafafa inset, 0 1px 1px 0 #dddddd !important;
    -moz-box-shadow: 0 0 0 1px #fafafa inset, 0 1px 1px 0 #dddddd !important;
    /* box-shadow: 0 0 0 1px #fafafa inset, 0 1px 1px 0 #dddddd; */
    padding: 0;
    display: block;
    font-weight: 700;
    /*white-space: nowrap;*/
    color: #626262;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.nuta-link-box-menu .ico-cirlce-widget {
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
    border: none;
    height: 40px;
}

.nuta-link-box-menu .ico-cirlce-widget span {
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
    background: none;
    border: none;

}

.nuta-link-box-menu .ico-cirlce-widget span i {
    color: #388E3C;
    font-size: 32px;
    line-height: 40px;
}

.nuta-link-box-menu .iconic-w-wrap .stat-w-title {
    text-transform: capitalize;
    display: block;
    padding: 5px;
    font-weight: 400;
    font-size: 18px;
    padding-top:20px;

}

.nuta-link-box-menu .w-meta-title {
    padding-left: 10px;
    padding-right: 10px;
    font-weight: 300;
}



.easyWizardSteps {list-style:none;width:100%;overflow:hidden;margin:0;padding:0;border-bottom:1px solid #ccc;margin-bottom:20px}
.easyWizardSteps li {font-size:18px;display:inline-block;padding:10px;color:#B0B1B3;margin-right:20px;}
.easyWizardSteps li span {font-size:18px}
.easyWizardSteps li.current {color:#000}
.easyWizardButtons {overflow:hidden;padding:10px;}
.easyWizardButtons button, .easyWizardButtons .submit {cursor:pointer}
.easyWizardButtons .prev {float:left}
.easyWizardButtons .next, .easyWizardButtons .submit {float:right}